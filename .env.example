# Example environment variables - Copy to .env and fill with actual values

# Database Configuration
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=your_db_name
POSTGRES_HOST=jware-postgres
POSTGRES_PORT=5432

# Redis Configuration
REDIS_PASSWORD=your_redis_password
REDIS_HOST=jware-redis
REDIS_PORT=6379

# Security Configuration
JWT_SECRET=your_jwt_secret
API_GATEWAY_SECRET=your_api_gateway_secret
ADMIN_PASSWORD=your_admin_password
ENCRYPTION_KEY=your_encryption_key

# Service URLs
TRADING_ENGINE_URL=http://jware-trading-engine:8000
MARKET_DATA_URL=http://jware-market-data:8001
API_GATEWAY_URL=http://jware-api-gateway:3000
WEB_UI_URL=http://jware-web-ui:3001

# Environment
NODE_ENV=development
PYTHON_ENV=development

# Trading API Keys (Add your keys here)
# Alpaca Trading
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets  # Use https://api.alpaca.markets for live trading

# Polygon.io Market Data
POLYGON_API_KEY=your_polygon_api_key_here

# Interactive Brokers (if using)
IB_GATEWAY_HOST=localhost
IB_GATEWAY_PORT=7497
IB_CLIENT_ID=1

# Binance (if using crypto)
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here

# Other potential providers
FINNHUB_API_KEY=your_finnhub_api_key_here
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
