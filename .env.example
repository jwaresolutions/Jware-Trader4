# Example environment variables - Copy to .env and fill with actual values

# Database Configuration
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=your_db_name
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Redis Configuration
REDIS_PASSWORD=your_redis_password
REDIS_HOST=redis
REDIS_PORT=6379

# Security Configuration
JWT_SECRET=your_jwt_secret
API_GATEWAY_SECRET=your_api_gateway_secret
ADMIN_PASSWORD=your_admin_password
ENCRYPTION_KEY=your_encryption_key

# Service URLs
TRADING_ENGINE_URL=http://trading-engine:8000
MARKET_DATA_URL=http://market-data:8001
API_GATEWAY_URL=http://api-gateway:3000
WEB_UI_URL=http://web-ui:3001

# Environment
NODE_ENV=development
PYTHON_ENV=development
